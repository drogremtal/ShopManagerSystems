@model IEnumerable<ShopManagerSystems.ViewModel.CheckViewModel>

@{
    ViewBag.Title = "Список чеков";
}

<a asp-action='CreateCheck'>Добавить пользователя</a>
<table class='table'>

        <tr>
            <th>ID</th>
            <th>Дата</th>
            <th>Сумма</th>
            <th>Файл</th>
            <th></th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Date</td>
                <td>@item.Summa</td>
                <td>@item.FileLink</td>
            </tr>
        }
</table>

